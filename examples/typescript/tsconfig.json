{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "types": [
      "@xiaohuohumax/bookmark-script/client"
    ],
    "target": "ES2019",
    "lib": ["ESNext", "DOM"],
    "declaration": false
  },
  "include": [
    "src/**/*",
    "types/**/*"
  ]
}